    <meta property="fb:app_id"          content="1907181596174570" /> 
    <meta property="og:type"            content="website" /> 
    <meta property="og:url"             content="http://www.murnow.com/" /> 
    <meta property="og:title"           content="Makeup Reviews Now" /> 
